<%- model_class = Legislation -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>
<table class="table table-striped">
  <tbody>
    <% @legislations.each do |legislation| %>
      <tr>
        <td><%= link_to legislation.id, legislation_path(legislation) %></td>
        <td><%= link_to legislation.titlu, legislation_path(legislation) %></td>
        <td><%= legislation.avatar %></td>
      </tr>
    <% end %>
  </tbody>
</table>