<%- model_class = Literature -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:Denumirea) %></th>
    </tr>
  </thead>
  <tbody>
    <% @literatures.each do |literature| %>
      <tr>
        <td><%= link_to literature.id %></td>
        <td><%= link_to literature.titlu, literature_path(literature) %></td>
      </tr>
    <% end %>
  </tbody>
</table>