<%- model_class = Subcategori -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:name) %>:</strong></dt>
  <dd><%= @subcategori.name %></dd>
  <dt><strong><%= model_class.human_attribute_name(:content) %>:</strong></dt>
  <dd><%= @subcategori.content %></dd>
  <dt><strong><%= model_class.human_attribute_name(:avatar) %>:</strong></dt>
  <dd><%= @subcategori.avatar %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              subcategoris_path, :class => 'btn btn-default'  %>
